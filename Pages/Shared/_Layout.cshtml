<!DOCTYPE html>
<html lang="en">
<head>
     <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EmployeeData</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Pages/Shared/_Layout.cshtml.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/EmployeeData.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    

</head>
<body id="Home">
    <div class=" icon-bar col-12">
  <a class="active col-3" href="/Registration/Registration" ><i class="fa fa-home"><span class="fa-label">Employee Portal</span> </i></a>
   <a class="col-3" href="/Registration/Registration"><i class="fa fa-user-plus" aria-hidden="true"><span class="fa-label">Register</span></i></a>
  <a class="col-3" href="/Registration/EmployeeList"><i class="fa fa-users" aria-hidden="true"> <span class="fa-label">View Employees</span></i></a> 
</div>
     
     <div class="container">
        <main role="main" >
             @RenderBody() 
            
        </main>
     </div>
   
<div>

</div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}